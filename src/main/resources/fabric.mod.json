{
  "schemaVersion": 1,
  "id": "skyfabrication",
  "version": "${version}",

  "name": "SkyFabrication",
  "description": "SkyFabrication is a Fabric mod aiming to provide a nice modded skyblock experience for the latest versions of Minecraft.",
  "authors": [
    "SimplyCmd"
  ],
  "contact": {
    "homepage": "https://simplycmd.github.io/SkyFabrication/",
    "sources": "https://github.com/SimplyCmd/SkyFabrication/"
  },

  "license": "CC0-1.0",
  "icon": "assets/skyfabrication/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.simplycmd.skyfabrication.Main"
    ],
    "client": [
      "io.github.simplycmd.skyfabrication.client.ClientMain"
    ]
  },
  "mixins": [
    "skyfabrication.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.17.x",
    "java": ">=16"
  },
  "suggests": {
    "another-mod": "*"
  }
}
