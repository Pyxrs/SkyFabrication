{
  "schemaVersion": 1,
  "id": "skyfabrication",
  "version": "${version}",

  "name": "SkyFabrication",
  "description": "A Fabric mod aiming to provide a nice modded skyblock experience for the latest versions of Minecraft.",
  "authors": [
    "SimplyCmd"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/skyfabrication/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.simplycmd.skyfabrication.Main"
    ]
  },
  "mixins": [
    "skyfabrication.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "minecraft": "1.17.x"
  },
  "suggests": {
    "another-mod": "*"
  }
}
